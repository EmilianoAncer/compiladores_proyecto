program patito;

def int fact(int j)
var int : i, p;
{
    i = j + (p - j * 2 + j);
    if (j == 1)
    {
        return j;
    }
    else
    {
        return (j * fact(j-1));
    }
}

def void inicia(int y)
var int : x;
arr int : arreglo[12];
{
    x = 1;
    while (x < 11)
    {
        arreglo[x] = y * x;
        x = x + 1;
    }
}

main()
var int : i, j, p;
arr int : arreglo[12], mat[12][9];
{   
